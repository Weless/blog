 \n          for(i=0;i<archiveList.length;i++){\n            var object = archiveList[i]\n            object.Year = object.Pub_time.slice(0,4)\n            object.Month = object.Pub_time.slice(5,-1)\n            this.archiveList.push(object)\n          }\n          // console.log(this.archiveList)\n        })\n      }\n    },\n    created(){\n      this.showArchives()\n    },\n    \n}\n</script>"]}]}