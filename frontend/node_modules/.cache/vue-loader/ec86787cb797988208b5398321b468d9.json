      </span>\n        <span class=\"views-count\">\n          <a href=\"#\">{{article_detail.ViewCount}} 阅读</a>\n        </span>\n      </div>\n    </header>\n    <div class=\"entry-content clearfix\">{{article_detail.Content}}</div>\n  </article>\n</template>\n\n<script>\nexport default {\n  name: \"article_detail\",\n  data() {\n    return {\n      article_detail: {}\n    };\n  },\n  methods: {\n    getArticleDetail() {\n      // console.log(this.$route)\n      var articleId = this.$route.params.articleId;\n      this.$http\n        .get(\"http://localhost:8000/api/articles/detail/\" + articleId)\n        .then(res => {\n          // console.log(res)\n          this.article_detail = res.data.article[0];\n          // console.log(this.article_detail)\n        })\n        .catch(err => {\n          console.log(err);\n          console.log(err);\n        });\n    },\n    dealWithTime: function(t) {\n      t = t.slice(0, -1);\n      t = t.replace(\"T\", \" \");\n      return t;\n    }\n  },\n  created() {\n    this.getArticleDetail();\n  }\n};\n</script>"]}]}