   //       this.register();\n    //     } else {\n    //       this.$message.error(\"格式错误\");\n    //     }\n    //   });\n    // },\n    register() {\n      // 验证数据\n      // this.$v.user.$touch(); // 即使表单为空也会报错\n      // if (this.$v.user.$anyError) {\n      //   // 表单验证出错直接返回\n      //   return;\n      // }\n      // 请求\n      this.userRegister(this.formItem)\n        .then(() => {\n          this.$router.replace({ path: \"/admin\" });\n          this.$message(\"注册成功\")\n        })\n        .catch(err => {\n          this.$message.error(\"登录失败！\" + err.response.data.msg)\n        });\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n.login-wrapper {\n  box-sizing: border-box;\n  position: absolute;\n  top: 100px;\n  left: 50%;\n  padding: 40px 40px;\n  width: 500px;\n  height: 350px;\n  border-radius: 4px;\n  margin-left: -250px;\n  box-shadow: 0 0 10px #ccc;\n  .login-title {\n    margin-bottom: 40px;\n  }\n  .login-input {\n    margin: 0 auto;\n  }\n}\n</style>\n"],"sourceRoot":"src/views/register"}]}